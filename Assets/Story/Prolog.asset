    characterSpr: {fileID: 21300000, guid: 374b88e30482a504aa58ca72dfb41ffb, type: 3}
    characterName: 
    dialogueText: "Pengejaran terus dilakukan bahkan dibantu oleh prajurit menantu
      Adipati Mersi. Raden Kamandaka lari ke arah timur menuju kota Purwokerto. Samapai
      di suatu tempat, Raden Kamandaka jatuh dan terluka. Tempat dimana dia jatuh
      akhirna dinamakan sungai \u201CBodas\u201D."
    backgroundSpr: {fileID: 0}
    startNode: 0
