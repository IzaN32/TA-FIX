  - guid: df31a26c-7e1c-413b-ab87-f39f8a457fbf
    characterSpr: {fileID: 21300000, guid: 9b2d087fc7f40b645a5c4a9dbb978aff, type: 3}
    characterName: 
    dialogueText: "Mereka mengejar Raden Kamandaka sampai disuatu daerah , karena
      kemarahannya seperi banteng ketaton, maka daerah itu dinamakan desa \u201CKedungbanteng\u201D."
